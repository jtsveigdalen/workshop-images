                if (url === undefined || url === null || !url.trim().length || url.indexOf('//www.youtube') === -1) {
                    return url;
                }

                var parts = url.split('/');
                parts[2] = parts[2].replace('.com', '-nocookie.com');
                return parts.join('/');
            }
