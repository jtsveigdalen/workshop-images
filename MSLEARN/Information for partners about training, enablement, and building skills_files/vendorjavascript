            searchTrackPosition: function searchTrackPosition(tracks, currentTime) {
                var lo = 0,
                    hi = tracks.length - 1,
                    mid = void 0,
                    start = void 0,
                    stop = void 0;

                while (lo <= hi) {
                    mid = lo + hi >> 1;
                    start = tracks[mid].start;
                    stop = tracks[mid].stop;

                    if (currentTime >= start && currentTime < stop) {
                        return mid;
                    } else if (start < currentTime) {
                        lo = mid + 1;
                    } else if (start > currentTime) {
                        hi = mid - 1;
                    }
                }

                return -1;
            }
