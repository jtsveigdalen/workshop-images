        function debounce(func, wait) {
            var _this = this,
                _arguments = arguments;

            var immediate = arguments.length > 2 && arguments[2] !== undefined ? arguments[2] : false;


            if (typeof func !== 'function') {
                throw new Error('First argument must be a function');
